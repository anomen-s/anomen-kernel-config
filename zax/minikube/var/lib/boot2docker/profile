export "NO_PROXY=.XXX.com"

export http_proxy=http://192.168.99.102:3129
export https_proxy=${http_proxy}
export HTTP_PROXY=${http_proxy}
export HTTPS_PROXY=${http_proxy}
                              
echo 192.168.99.102 zax >> /etc/hosts
 
ifconfig eth1 192.168.99.100 netmask 255.255.255.0

gunzip < /var/lib/boot2docker/certs.d.tgz | tar -x -C /etc/docker

