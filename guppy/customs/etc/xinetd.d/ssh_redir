# ssh redir as pop3s
service pop3s
{
	disable		= no
	id		= ssh-redir
	socket_type	= stream
	protocol	= tcp
	user		= root
	wait		= no
	redirect 	= 127.0.0.1 10022
}                                                                               

service https
{
	disable		= no
	id		= ssh-redir
	socket_type	= stream
	protocol	= tcp
	user		= root
	wait		= no
	redirect 	= 127.0.0.1 10022
}                                                                               

